<service role="NIFICONTENTVIEWER" name="nifi-content-viewer" version="0.0.1">
    <policies>
        <policy role="webappsec"/>
        <policy role="authentication" name="Anonymous"/>
        <policy role="rewrite"/>
    </policies>
    <routes>
        <route path="/nifi-content-viewer/**?**">
        </route>
        <route path="/nifi-content-viewer">
        </route>
        <route path="/nifi-content-viewer/**">
        </route>
    </routes>
    <dispatch classname="org.apache.hadoop.gateway.dispatch.PassAllHeadersDispatch"/>
</service>

